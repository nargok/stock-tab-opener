{
  "manifest_version": 3,
  "name": "証券コード一括オープナー (TS)",
  "version": "1.0",
  "description": "証券コードを基に、ヤフーファイナンス、株探、四季報オンラインを新しいタブで開きます。",
  "permissions": [
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}